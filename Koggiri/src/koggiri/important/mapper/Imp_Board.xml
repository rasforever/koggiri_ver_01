<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="koggiri.important.mapper.Imp_BoardMapper">
	<cache />
	<insert id="insert_ImpBoard" parameterType="Imp_Board">
		insert into importantboard(
		i_id, i_emp_id, i_title, i_content, i_date,
		i_hit
		)
		values(
		nboard.nextval, #{i_emp_id}, #{i_title}, #{i_content},
		SYSDATE, 0
		)
	</insert>
	
<select id="Imp_listBoard" resultType="Imp_Board">
		select * from importantboard
		</select>



</mapper>