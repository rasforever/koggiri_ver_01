<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="koggiri.change_pw.mapper.ChangePwMapper"> <!--네임스페이스는 매퍼경로와 같게 해줘야한다.  -->
	
	<select id="member" resultType="ChangePw"> <!-- 로그인한 아이디에 맞는 비밀번호 가지고오기-->
		select mem_id, mem_pw from MEMBER
	</select>
	
	<insert id="newPw" >
		insert mem_pw from MEMBER
	</insert>

</mapper>















